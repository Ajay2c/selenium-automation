	</test>
	
	<test name="CorrectPassword">
		<parameter name="username" value="Ashok">
		</parameter>
		<parameter name="password" value="K38eXty@@@DJ">
		</parameter>
		<classes>
			<class name="loginTestcases.correctPassword">
			</class>
		</classes>
	</test>
	
	<test name="CorrectUsername">
		<parameter name="username" value="Admin">
		</parameter>
		<parameter name="password" value="123456">
		</parameter>
		<classes>
			<class name="loginTestcases.correctUserName">
			</class>
		</classes>
	</test>
	
</suite>	
	
